<?xml version="1.0" encoding="UTF-8"?>
<!--
    개인설정
    default-config.xml의 값과 중복되는 것은 user-config.xml의 값이 우선된다.
-->
<config>
    <!-- debug / release -->
    <mode>
        <type>debug</type>
    </mode>

    <!-- 화면정보 -->
    <viewinfo>
        <language>en</language>
    </viewinfo>

    <!-- 파일 업로드 및 저장 경로 설정 -->
    <path>
        <base>/Users/Server/app_resources</base>
        <temp>${path.base}/temp</temp>
    </path>
</config>